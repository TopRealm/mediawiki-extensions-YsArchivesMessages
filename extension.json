{
	"name": "YsArchivesMessages",
	"author": [
		"Tim Starling",
		"Siebrand Mazeland",
		"James D. Forrester",
		"Multichill",
		"[https://github.com/ZoruaFox ZoruaFox]"
	],
	"url": "https://github.com/TopRealm/mediawiki-extensions-YsArchivesMessages",
	"namemsg": "ysarchivesmessages",
	"descriptionmsg": "ysarchivesmessages-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43"
	},
	"MessagesDirs": {
		"YsArchivesMessages": [
			"i18n/extensions",
			"i18n/cclicensetexts",
			"i18n/usergroups"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\YsArchivesMessages\\": "includes/"
	},
	"ResourceModules": {
		"ext.ysarchivesmessages": {
			"packageFiles": [ "ext.YsArchivesMessages.js" ]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "YsArchivesMessages/resources"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\YsArchivesMessages\\Hooks",
			"services": [ "MainConfig" ]
		}
	},
	"Hooks": {
		"BeforePageDisplay": {
			"handler": "main"
		}
	},
	"manifest_version": 2
}
